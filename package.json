{
  "name": "OCARHAApp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android:dev": "react-native run-android --mode=devDebug",
    "android:stage": "react-native run-android --mode=stagingDebug",
    "android:prod": "react-native run-android --mode=productionDebug",
    "ios": "react-native run-ios",
    "android": "react-native run-android",
    "clean-android-assets": "rm -rf ./android/app/src/main/res/drawable-*; rm -rf ./android/app/src/main/res/raw-*",
    "clean-android-build": "cd android && ./gradlew clean",
    "build-android-stage": "cd android && ./gradlew assembleRelease",
    "build-android-prod": "cd android && ./gradlew clean assembleProductionRelease",
    "bundle:prod": "cd android && ./gradlew clean bundleProductionRelease && ./gradlew bumpProdVersionCode",
    "bundle:dev": "cd android && ./gradlew bumpDevVersionCode && ./gradlew clean bundleDevRelease ",
    "bundle:staging": "cd android && ./gradlew bumpStagingVersionCode && ./gradlew clean bundleStagingRelease ",
    "ios-stage": "react-native run-ios --scheme Stage",
    "ios-prod": "react-native run-ios --scheme production",
    "lint": "eslint . --ext .tsx",
    "start": "react-native start",
    "test": "jest",
    "postinstall": "patch-package",
    "android:bundle": "cd android && ./gradlew clean bundleRelease",
    "clean-cache-android-ios-build": "watchman watch-del-all && rm -rf $TMPDIR/react-* $TMPDIR/metro-bundler-cache-*  $TMPDIR/react-native-packager-cache-* rm -rf node_modules/ ios/Podfile.lock ios/Pods ios/build android/app/build",
    "x86-node-modules-pod-install": "arch -x86_64 yarn install && cd ios && arch -x86_64 pod install"
  },
